{
	"log": {
		"access": "/dev/null",
		"error": "/dev/null",
		"loglevel": "warning"
	},
	"inbounds": [
		{
			"port": 10000,
			"listen": "127.0.0.1",
			"protocol": "vmess",
			"settings": {
				"clients": [
					{
						"id": "UUID",
						"alterId": 0
					}
				]
			},
			"streamSettings": {
				"network": "xhttp",
				"security": "reality",
				"realitySettings": {
					"show": false,
					"dest": "www.example.com:443",
					"serverNames": ["www.example.com"],
					"privateKey": "YOUR_PRIVATE_KEY",
					"shortIds": ["YOUR_SHORT_ID"]
				},
				"xhttpSettings": {
					"path": "VMESS_WSPATH",
					"mode": "packet" // 或 "auto" / "stream"
				}
			},
			"sniffing": {
				"enabled": true,
				"destOverride": ["http", "tls"]
			}
		},
		{
			"port": 20000,
			"listen": "127.0.0.1",
			"protocol": "vless",
			"settings": {
				"clients": [
					{
						"id": "UUID"
					}
				],
				"decryption": "none"
			},
			"streamSettings": {
				"network": "xhttp",
				"security": "reality",
				"realitySettings": {
					"show": false,
					"dest": "www.example.com:443",
					"serverNames": ["www.example.com"],
					"privateKey": "YOUR_PRIVATE_KEY",
					"shortIds": ["YOUR_SHORT_ID"]
				},
				"xhttpSettings": {
					"path": "VLESS_WSPATH",
					"mode": "packet" // 或 "auto" / "stream"
				}
			},
			"sniffing": {
				"enabled": true,
				"destOverride": ["http", "tls"]
			}
		},
		{
			"port": 30000,
			"listen": "127.0.0.1",
			"protocol": "trojan",
			"settings": {
				"clients": [
					{
						"password": "UUID"
					}
				]
			},
			"streamSettings": {
				"network": "xhttp",
				"security": "reality",
				"realitySettings": {
					"show": false,
					"dest": "www.example.com:443",
					"serverNames": ["www.example.com"],
					"privateKey": "YOUR_PRIVATE_KEY",
					"shortIds": ["YOUR_SHORT_ID"]
				},
				"xhttpSettings": {
					"path": "TROJAN_WSPATH",
					"mode": "packet" // 或 "auto" / "stream"
				}
			},
			"sniffing": {
				"enabled": true,
				"destOverride": ["http", "tls"]
			}
		},
		{
			"port": 40000,
			"listen": "127.0.0.1",
			"protocol": "shadowsocks",
			"settings": {
				"clients": [
					{
						"method": "chacha20-ietf-poly1305",
						"password": "UUID"
					}
				]
			},
			"streamSettings": {
				"network": "xhttp",
				"security": "reality",
				"realitySettings": {
					"show": false,
					"dest": "www.example.com:443",
					"serverNames": ["www.example.com"],
					"privateKey": "YOUR_PRIVATE_KEY",
					"shortIds": ["YOUR_SHORT_ID"]
				},
				"xhttpSettings": {
					"path": "SS_WSPATH",
					"mode": "packet" // 或 "auto" / "stream"
				}
			},
			"sniffing": {
				"enabled": true,
				"destOverride": ["http", "tls"]
			}
		}
	],
	"outbounds": [
		{
			"protocol": "freedom",
			"settings": {},
			"tag": "direct"
		},
		{
			"protocol": "blackhole",
			"settings": {},
			"tag": "block"
		}
	],
	"routing": {
		"domainStrategy": "IPIfNonMatch",
		"rules": [
			{
				"type": "field",
				"ip": ["geoip:private"],
				"outboundTag": "block"
			}
		]
	},
	"dns": {
		"servers": [
			"8.8.8.8",
			"8.8.4.4",
			"localhost"
		]
	}
}
